    <script>
        try {

            //radar chart
            var ctx = document.getElementById("faculty");
            if (ctx) {
                ctx.height = 200;
                var myChart = new Chart(ctx, {
                    type: 'radar',
                    data: {
                        labels: [["Teaching", "Ability"], ["Exercise", "Eficiency"], "Teacher-Ethics", ["Specialize", "Talent"], "Assidous"],
                        defaultFontFamily: 'Poppins',
                        datasets: [
                            {
                                label: "Overall",
                                data: [@ViewBag.feedback.Teaching, @ViewBag.feedback.Exercises, @ViewBag.feedback.TeacherEthics, @ViewBag.feedback.Specialize, @ViewBag.feedback.Assiduous],
                                borderColor: "rgba(0, 123, 255, 0.6)",
                                borderWidth: "1",
                                backgroundColor: "rgba(0, 123, 255, 0.4)"
                            }
                        ]
                    },
                    options: {
                        legend: {
                            position: 'top',
                            labels: {
                                fontFamily: 'Poppins'
                            }

                        },
                        scale: {
                            ticks: {
                                beginAtZero: true,
                                fontFamily: "Poppins"
                            }
                        }
                    }
                });
            }

        } catch (error) {
            console.log(error)
        }


    </script>

