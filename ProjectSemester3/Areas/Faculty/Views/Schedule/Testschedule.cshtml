@{
    ViewData["Title"] = "Test Schedule";
    Layout = "~/Areas/Faculty/Views/Shared/_Layout.cshtml";
}
<div class="section__content section__content--p30">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="overview-wrap">
                    <h2 class="title-1">Test Schedule</h2>

                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <table class="table table-borderless table-striped table-earning">
                    <thead>
                        <tr>
                            <th>Class Name</th>
                            <th>Exam Name</th>
                            <th>Time</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var schedule in ViewBag.testschedules)
                        {
                            <tr>
                                <td>@schedule.Class.ClassName</td>
                                <td>@schedule.Exam.Title</td>
                                <td>@schedule.Date</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>