@{
    ViewData["Title"] = "Studentbyclass";
    Layout = "~/Areas/Faculty/Views/Shared/_Layout.cshtml";
}
<div class="section__content section__content--p30">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="overview-wrap">
                    <h2 class="title-1">Roll Call</h2>

                </div>
            </div>
        </div>
        <div class="row m-t-25">
            <form action="/" method="post">
                <table class="table table-hover">
                    <thead>
                        <tr>
                            <th>Class Name</th>
                            <th>Subject Name</th>
                            <th>Time</th>
                            <th>Start Day</th>
                            <th>End Day</th>
                            <th>Study Day</th>

                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var schedule in ViewBag.schedules)
                        {
                            <tr>
                                <td>@schedule.Class.ClassName</td>
                                <td>@schedule.Subject.SubjectName</td>
                                <td>@schedule.TimeDay</td>

                                <td> @schedule.StartDate</td>


                                <td>@schedule.EndDate</td>
                                <td>@schedule.StudyDay</td>

                               
                            </tr>

                        }


                    </tbody>
                </table>
            </form>
        </div>

    </div>
</div>